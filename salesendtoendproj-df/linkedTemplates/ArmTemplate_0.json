		{
			"name": "[concat(parameters('factoryName'), '/ds_s3_orderItems')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_AmazonS3",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Json",
				"typeProperties": {
					"location": {
						"type": "AmazonS3Location",
						"bucketName": "retail-project1",
						"fileName": "order_items.json"
					}
				},
				"schema": {
					"type": "object",
					"properties": {
						"order_item_id": {
							"type": "integer"
						},
						"order_item_order_id": {
							"type": "integer"
						},
						"order_item_product_id": {
							"type": "integer"
						},
						"order_item_quantity": {
							"type": "integer"
						},
						"order_item_subtotal": {
							"type": "number"
