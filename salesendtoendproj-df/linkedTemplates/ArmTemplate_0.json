		"ls_adls_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://salesendtoendprojdlsa.dfs.core.windows.net/"
		},
		"ls_AmazonS3_properties_typeProperties_accessKeyId": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "ls_AzureKeyVault",
					"type": "LinkedServiceReference"
				},
				"secretName": "S3AccessKeyID"
			}
		},
		"ls_sql_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "salesendtoend-db-server.database.windows.net"
		},
		"ls_sql_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "salesendtoend_db"
		},
