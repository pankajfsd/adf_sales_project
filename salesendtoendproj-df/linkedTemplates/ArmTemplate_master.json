{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "salesendtoendproj-df"
		},
		"ls_adls_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_ls'"
		},
		"ls_AmazonS3_properties_typeProperties_accessKeyId": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "ls_AzureKeyVault",
					"type": "LinkedServiceReference"
				},
				"secretName": "S3AccessKeyID"
			}
		},
		"ls_AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://salesproject-kvs.vault.azure.net/"
		},
		"ls_adls_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://salesendtoendprojdlsa.dfs.core.windows.net/"
		},
		"ls_sql_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "salesendtoend-db-server.database.windows.net"
		},
		"ls_sql_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "salesendtoend_db"
		},
		"ls_sql_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "salesdb"
		},
		"tr_sales_project_properties_pl_sales_project_parameters_fileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"tr_sales_project_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/c9939a34-52b0-4a0a-9cb5-6a03f1daff58/resourceGroups/sales-endtoend-proj-rg/providers/Microsoft.Storage/storageAccounts/salesendtoendprojdlsa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "salesendtoendproj-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_ls_accountKey": {
						"value": "[parameters('ls_adls_ls_accountKey')]"
					},
					"ls_AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"ls_adls_ls_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_ls_properties_typeProperties_url')]"
					},
					"ls_AmazonS3_properties_typeProperties_accessKeyId": {
						"value": "[parameters('ls_AmazonS3_properties_typeProperties_accessKeyId')]"
					},
					"ls_sql_db_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_db_properties_typeProperties_server')]"
					},
					"ls_sql_db_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_db_properties_typeProperties_database')]"
					},
					"ls_sql_db_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_db_properties_typeProperties_userName')]"
					},
					"tr_sales_project_properties_pl_sales_project_parameters_fileName": {
						"value": "[parameters('tr_sales_project_properties_pl_sales_project_parameters_fileName')]"
					},
					"tr_sales_project_properties_typeProperties_scope": {
						"value": "[parameters('tr_sales_project_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}